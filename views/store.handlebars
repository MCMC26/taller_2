                                    <img  class="miniature" src="/media/notebooks/{{color_images.[0].images.[1]}}.png" alt="">
                                    <img class="miniature" src="/media/notebooks/{{color_images.[0].images.[2]}}.png" alt="">
                                
                            </div>
                        </div>
                        
                    </div>
                
                {{/if}}
        {{/each}}    

    </div>
