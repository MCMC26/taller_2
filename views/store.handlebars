        <div class="publicity">
            {{#each products}}
                {{#if publicity}}
                    <div class="publicity_left">
                        <h1 class="publicity_tittle">
                        {{{name}}}
                        </h1>
                        <h2 class="publicity_subtittle">
                            NOTEBOOKS
                        </h2 >
                    
                        <p class="publicity_text">
                        {{{description}}} 
                        </p>
                        <div class="publicity_price">
                            <P class="publicity_priceb">
                            ${{{priceb}}} 
                            </p>
                            <P>
